{
    static void Main(string[] args)
    {
        var d = new a();
        d.num1 = 45;
        d.num2 = 67;
        d.add();
      //  Console.WriteLine(d.add2 );
        Console.WriteLine(d.add4=89);





    }
    

}
class a 
{
    private int number_1;
    private int number_2;
    private int result;
    private int s;
    private int l;
    public int num1
    {
        set 
        {
            number_1 = value;
        }
        get
        {
            return number_1;
        }

    }
    public int num2
    {
        set
        {
            number_2 = value;
        }
        get
        {
            return number_2;
        }

    }
    public void add()
    {
        result = number_1+number_2;
        

    }
    public int add2
    {
        get { return result; }
    }
    public int add3
    {
        set { s=value; }
    }
    public int add4 { get; set; }

}
